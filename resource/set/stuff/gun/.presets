(define "damage"            ;changed the scale to simulate close range tank combat where penetration remains high at shorter distances                                                              
	{projectileDamageTable {20 %a} {100 %b} {170 %c} {215 %d} {250 %e} {255 0} }
	;{projectileDamageTable {15 %a} {80 %b} {140 %c} {195 %d} {250 %e} {(* 250 2) (* %e 0.5)} {(* 250 4) (* %e 0)}}

	;IRL range:					10m  - 500m  - 1000m - 1500m  - 2000m
	;MOW range:		        	0m   - 62.5m - 125m  - 187.5m - 250m
	;% of the curve				0%   - 25%   - 50%   - 75%    - 100%
	;% of next meter measure:	0%   - 50%   - 66%   - 75%    - 100%
)
(define "damage_215"            ;changed the scale to simulate close range tank combat where penetration remains high at shorter distances                                                              
	{projectileDamageTable {20 %a} {100 %b} {170 %c} {215 %d} {%range (+ (* (/ (- 250 %range) (- 250 215)) (- %d %e)) %e)} {(+ %range 5) 0} }
)
(define "damage_170"            ;changed the scale to simulate close range tank combat where penetration remains high at shorter distances                                                              
	{projectileDamageTable {20 %a} {100 %b} {170 %c} {%range (+ (* (/ (- 215 %range) (- 215 170)) (- %c %d)) %d)} {(+ %range 5) 0} }
)
(define "damage_100"            ;changed the scale to simulate close range tank combat where penetration remains high at shorter distances                                                              
	{projectileDamageTable {20 %a} {100 %b} {%range (+ (* (/ (- 170 %range) (- 170 100)) (- %b %c)) %c)} {(+ %range 5) 0} }
)

(define "damage_heat"                                                                                 
