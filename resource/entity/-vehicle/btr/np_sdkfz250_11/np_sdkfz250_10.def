{entity
	(include "/properties/armoredcar.ext")
	(include "/properties/selection/vehicle.inc" scale(1.0))
	{props "vehicle" "armored_car" "tank" "turret" "wheeled_8" "with_gun" "explosion_throw_crew" "vision_cannon_medium" "muzzle_small"}
	{targetClass "armoredcar" "ap fg bullet"}
	{targetSelector "tank"}
	{Extension "sdkfz250_10.mdl"}

	{boarder
		{anm "passengers"
			{forward	{begin	"board_SdKfz251-1"} {end "seat_passenger"} {base "close"}}
			{reverse 	{end	"emit_tank_1"} {base "close" -1}}
		}
		{anm "passengers1"
			{forward	{begin	"board_SdKfz251-1"} {end "seat_passenger"} {base "close"}}
			{reverse 	{end	"emit_body"} {base "close" -1}}
		}
		{anm "driver"
			{forward	{begin	"board_SdKfz251-1"} {end "seat_driver_car"} {base "close"}}
			{reverse 	{end	"emit_body" 1} {base "close" -1}}
		}
		{anm "gunner"
			{forward	{begin	"board_SdKfz251-1"} {end "seat_sd251_mgunner"} {base "close"}}
			{reverse 	{end	"emit_body" 1} {base "close" -1}}
		}
	}

	(define "crew_human"
		{placer
			{place "%place" {group "%group"} {linkbone "%bone"} {turnoff {sensor} {shadow}}}
		}
		{boarder
			{door "%door"}
			{link "%door" "%place" {anm "%animation"} {forward putoff} {reverse puton}}
		}
	)
	{sensor
		{visor "gunner1" {bone "foresight3"}}
	}
	{placer
		{place "driver" {visor "driver"}}
		{place "gunner" {visor "main"}}
		{place "gunner" {useVision} {visor "gunner1"}}
	}

	("crew_human" place(driver)	door(emit1) bone(driver) animation(driver)		group(crew))
	("crew_human" place(commander)	door(emit1) bone(commander) animation(driver)		group(crew))
	("crew_human" place(gunner)	door(emit1) bone(gunner) animation(gunner)		group(crew))
	("crew_human" place(gunner1)	door(emit1) bone(gunner1) animation(gunner)		group(crew))
	("crew_human" place(seat2)	door(emit1) bone(seat02) animation(passengers1)	group(crew))
	("crew_human" place(seat1)	door(emit1) bone(seat01) animation(passengers)	group(crew))
	
	

	{volume "body"
		{tags "body"}
		{component "body"}
		{thickness 8}   
	}
	{volume "body1"
		{tags "body"}
		{component "body"}
		{thickness 14.5}   
	}
	{volume "body2"
		{tags "body"}
		{component "body"}
		{thickness 8}   
	}
	{volume "body3"
		{tags "body"}
		{component "body"}
		{thickness 8}   
	}
	{volume "body4"
		{tags "body"}
		{component "body"}
		{thickness 8}   
	}
	{volume "body5"
		{tags "body"}
		{component "body"}
		{thickness 8}   
	}
	{volume "engine"
		{tags "engine"}
		{component "engine"}
		{thickness 8
			{front	14.5}
		}
	}
	{volume "turret"
		{tags "turret"}
		{component "turret"}
		{thickness 8}
	}
	{volume "gun"
		{thickness 8}
	}
	{component "gun"
		{RepairTime 20}
	}
	{volume "gun_rot"
		{tags "gun"}
		{component "gun"}
		{thickness 8}
	}
	("armor_medium" args 1.0)
	 {Placer
	          {place "attach01"
	             {entity "mauzer"}
	             {Linkbone "attach01"}
	             {handlebone "basis"}
	             {obstacle disable}
	          }
	          {place "attach02"
	                  {entity "mauzer"}
	                   {Linkbone "attach02"}
	                   {handlebone "basis"}
	                   {obstacle disable}
	          }
	          {place "attach03"
	                   {entity "mauzer"}
	                   {Linkbone "attach03"}
	                   {handlebone "basis"}
	                   {obstacle disable}
	          }
	          {place "attach04"
	                   {entity "mauzer"}
	                   {Linkbone "attach04"}
	                   {handlebone "basis"}
	                   {obstacle disable}
	          }
	}
	 (define "define_place"
	          {placer
	             {place "%place"
	                {linkbone "%place"}
	                {attach squat_lie}
	                {turnoff {shadow}}
	                {obstacle disable}
	             }
	          }
	          {Volume "%place"
	             {tags "attach" "attach_%place"}
	             {able clear {bullet 0} {touch 1}}
	          }
	)
	("wheel" args "wheelright")
	("wheel" args "wheelleft")
	("tracks")

	{extender "enumerator"
		{digit_folder "ger"}
	}
	{extender "inventory"
		{box
			{item "bullet37 ger ap" 50}
			{item "bullet37 ger fg" 50}
			{item "ammo mg_mauser drum" 1100}
		}
	}
	{Weaponry
		{place "gun"
			{weapon "37mm_pak35" filled}
			{RestoreIKAfterAim}
			{charger "gunner"}
			{basic}
		}
		{place "mgun"
			{type "mgun_main"}
			{LinkBone "mgun"}
			{LinkAnimation "off"}
			{weapon "mg34" filled}
			{gunner "gunner1"}
			{charger "gunner1"}
			{foresight "foresight3"}
			{basic}
		}
	}  	
		
	{Chassis
		{Locomotion ("locomotion")
               	 	{MaxSpeed	38}	
	  		{maxSpeedAtMaxTurn 6}	
			{StartTime	20}
			{BrakeTime	1.5}	
			{StopTime	2.5}
			{TurnRadius	8}
     			{Gears  0.1 0.2 0.3 1}
			{RearGears  0.25}
		}
			{FuelBag
				{volume 200 }
				{rate	 10 } 
				{remain 200 }
				{fuel "fuel"}
			}
		    {speed
        		{Normal  32}
    		}
		; left track must be first
		("tank_trace" fx("tracks_mid") step(0.5) len(0.86) boneleft("FXtraceL2") boneright("FXtraceR2"))
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
	}
	{mass 8200}
	       
}
